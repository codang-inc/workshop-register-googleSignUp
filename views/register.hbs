<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link href="css/register.css" rel="stylesheet" type="text/css" />
    <link
        href="https://fonts.googleapis.com/css2?family=Chilanka&family=Lato&family=Quicksand&family=Raleway:wght@300;700&display=swap"
        rel="stylesheet" />
    <link rel="stylesheet" type="text/css" href="//fonts.googleapis.com/css?family=Open+Sans" />

    <title>Register</title>
</head>

<body>

    <body>
        <span id="root">
            <section class="section-all">
                <!-- 1-Role Main -->
                <main class="main" role="main">
                    <div class="wrapper">
                        <article class="article">
                            <div class="content">
                                <div class="login-box">
                                    <div class="header">Workshop Registeration</div>
                                    <div class="but-container">
                                        <a href="/auth/google" class="google-btn">
                                            <div class="google-icon-wrapper">
                                                <img class="google-icon"
                                                    src="https://upload.wikimedia.org/wikipedia/commons/5/53/Google_%22G%22_Logo.svg" />
                                            </div>
                                            <p class="btn-text"><b>Register with google</b></p>
                                        </a>
                                    </div>
                                </div>
                                <div class="login-box">
                                    <a href="/users" class="text">
                                        <button class="btn" type="button">
                                            See all registered students
                                        </button>
                                    </a>
                                </div>
                            </div>
                        </article>
                    </div>
                </main>
            </section>
        </span>
        <!-- Root -->

        <!-- Select Link -->
        <script type="text/javascript">
            function la(src) {
                window.location = src;
            }
        </script>
    </body>
</body>

</html>